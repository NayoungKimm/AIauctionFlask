		url = url + '&mc='+G_mc;
		window.open(url,'newwin','');
	}
	else
	{
		document.location.href = url;
	}

 }

/*************************************************************
		function mouseMemo,mouseMemx(번호)
		용도    : 툴팁
**************************************************************/
function mouseMemo(num) {
	eval('tooltip'+num).style.visibility = "visible";  // 툴팁 보이기
}
function mouseMemx(num) {
	eval('tooltip'+num).style.visibility = "hidden";  // 툴팁 감추기
}

/*************************************************************
		function detail_view(링크주소,법원코드,사건년도,사건번호,물건번호,??)
		용도    : 사건상세페이지
**************************************************************/
function detail_view(var1,var2,var3,var4, authkey)
{
	var f = document.result_switch;

	var style = "Left=0,Top=0,width=874,height=700,scrollbars=yes";
	window.open("about:blank",'resultSwitch',style);
	f.target = "resultSwitch";

	f.courtNo.value = var1;
	f.eventNo1.value = var2;
	f.eventNo2.value = var3;
	f.objNo.value = var4;
	f.auth_key.value = authkey;

	f.submit();
}

