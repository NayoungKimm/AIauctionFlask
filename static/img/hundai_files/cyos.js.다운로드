		var item = new Option('강릉');
		document.info.courtNo_sub.options[5] = item;
		document.info.courtNo_sub.options[5].value='F02';
		document.info.courtNo_sub.options[5].text='강릉';
		break;
	}
}

/**************************************************************************
법원선택
**************************************************************************/
function set_court2(frm_name,main_courtno)
{
	var f = eval("document."+frm_name+".courtNo_sub");
	
	//법원	
	f.options[9] = null;
	f.options[8] = null;
	f.options[7] = null;
	f.options[6] = null;
	f.options[5] = null;
	f.options[4] = null;
	f.options[3] = null;
	f.options[2] = null;
	f.options[1] = null;
	f.options[0] = null;		
	switch(main_courtno)
	{
		case "0" : //본원전체		
		var item = new Option('지원전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='지원전체';		
		break;
		
		case "A00" : //서울지법전체
		var item = new Option('전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='전체';
		break;
		
		case "A01" : //서울중앙
		var item = new Option('전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='전체';
		break;
		
		case "A02" : //서울동부
		var item = new Option('전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='전체';
		break;
		
		case "A03" : //서울서부
		var item = new Option('전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='전체';
		break;
		
		case "A04" : //서울남부
		var item = new Option('전체');
		f.options[0] = item;
		f.options[0].value=99;
		f.options[0].text='전체';
		break;
		
		case "A05" : //서울북부
